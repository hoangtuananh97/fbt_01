<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="app.model">
    <class name="Placeimage" table="placeimage" catalog="db_bookingtour" optimistic-lock="version">
        <id name="id" type="int">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <many-to-one name="place" class="Place" fetch="select">
            <column name="places_id" not-null="true" />
        </many-to-one>
        <property name="urlImage" type="string">
            <column name="urlImage" length="245" />
        </property>
    </class>
</hibernate-mapping>
